<?xml version="1.0"?>
<settings>
    <!-- MIDI OUTPUT -->
    <nb_ignored_startup_ticks>0</nb_ignored_startup_ticks>
	<midi_outputs>
		<output>
			<name>Sceno</name>
			<device_id>IAC Driver Bus 1</device_id>
			<send_ticks>1</send_ticks>
			<send_timecodes>1</send_timecodes>
		</output>
		<output>
			<name>MC</name>
			<device_id>Elektron Model:Cycles</device_id>  <!--device_id: full or partial name as it appears in the OS-->
			<channel>10</channel>  <!--channel: 0, 1, 2, ...-->
			<!--input format options:-->
			<!--elektron=Elektron format (eg B06)-->
			<!--patch_name=resolves program from patch name in this config-->
			<input_format>elektron_pattern</input_format>
			<use_legacy_program>1</use_legacy_program>  <!--this device should use legacy 'program' field from song-->
			<send_ticks>1</send_ticks>
		</output>
        <output>
			<name>Digitakt</name>
			<device_id>Elektron Digitakt</device_id>
			<channel>10</channel>
			<input_format>elektron_pattern</input_format>
			<use_legacy_program>1</use_legacy_program>  <!--this device should use legacy 'program' field from song-->
			<send_ticks>1</send_ticks>
		</output>
        <output>
            <name>ChantLead</name>
            <device_id>IAC Driver Bus 2</device_id>
            <channel>10</channel>
            <input_format>patch_name</input_format>
            <patches>
                <patch>
                    <name>clean</name>
                    <program>0</program>
                </patch>
                <patch>
                    <name>autotune</name>
                    <program>1</program>
                </patch>
                <patch>
                    <name>reverb</name>
                    <program>2</program>
                </patch>
            </patches>
        </output>
		<output>
			<name>ORGAN</name>
			<device_id>IAC Driver Bus 3</device_id>
			<channel>10</channel>
			<input_format>patch_name</input_format>
			<patches>
				<patch>
					<name>Pakela</name>
					<program>0</program>
				</patch>
				<patch>
					<name>AgeIdiot</name>
					<program>1</program>
				</patch>
				<patch>
					<name>Fuministe</name>
					<program>2</program>
				</patch>
				<patch>
					<name>Legume</name>
					<program>3</program>
				</patch>
				<patch>
					<name>UGV</name>
					<program>4</program>
				</patch>
				<patch>
					<name>Calliope</name>
					<program>5</program>
				</patch>
				<patch>
					<name>Church</name>
					<program>6</program>
				</patch>
				<patch>
					<name>THS</name>
					<program>7</program>
				</patch>
				<patch>
					<name>FeteForaine</name>
					<program>8</program>
				</patch>
				<patch>
					<name>DPALST</name>
					<program>9</program>
				</patch>
                <patch>
                    <name>Bardino</name>
                    <program>10</program>
                </patch>
                <patch>
                    <name>HeartBass</name>
                    <program>11</program>
                </patch>
                <patch>
                    <name>MDMC</name>
                    <program>16</program>
                </patch>
			</patches>
		</output>
        <output>
            <name>GTR</name>
            <device_id>IAC Driver Bus 4</device_id>
            <channel>10</channel>
            <input_format>patch_name</input_format>
            <patches>
                <patch>
                    <name>clean</name>
                    <program>0</program>
                </patch>
                <patch>
                    <name>disto</name>
                    <program>1</program>
                </patch>
            </patches>
        </output>
	</midi_outputs>

    <!-- SETLIST -->
	<songs_root_dir>./songs/</songs_root_dir>
    
    <auto_play_delay_seconds>2</auto_play_delay_seconds>
    
    <!-- BACKING TRACKS -->
    <ignore_audio_files>
        <containing><value>backup</value></containing>
        <containing><value>test</value></containing>
    </ignore_audio_files>
    
    <!-- VIDEO CONTROL  -->
    <video_start_delay_ms>40</video_start_delay_ms>
    <video_speed_change_delay_ms>20</video_speed_change_delay_ms>
    
    <!-- VIDEO WINDOW -->
	<window_pos_x>100</window_pos_x>
	<window_pos_y>200</window_pos_y>
	<window_pos_w>1500</window_pos_w>
	<window_pos_h>800</window_pos_h>
	<show_video_window>1</show_video_window>
	<enable_visuals>1</enable_visuals>
</settings>
